<?xml version="1.0" encoding="UTF-8"?>
<FBType Name="WORD2WORD" Comment="Writing WORD input to output">
	<Identification Standard="61499-1">
	</Identification>
	<VersionInfo Version="1.0" Author="Bianca Wiesmayr" Date="2023-03-07">
	</VersionInfo>
	<InterfaceList>
		<EventInputs>
			<Event Name="REQ" Type="Event" Comment="Normal Execution Request">
				<With Var="IN"/>
			</Event>
		</EventInputs>
		<EventOutputs>
			<Event Name="CNF" Type="Event" Comment="Execution Confirmation">
				<With Var="OUT"/>
			</Event>
		</EventOutputs>
		<InputVars>
			<VarDeclaration Name="IN" Type="WORD" Comment=""/>
		</InputVars>
		<OutputVars>
			<VarDeclaration Name="OUT" Type="WORD" Comment=""/>
		</OutputVars>
	</InterfaceList>
	<SimpleFB>
		<Algorithm Name="REQ" Comment="">
			<ST><![CDATA[ALGORITHM REQ
	OUT:=IN;
END_ALGORITHM

]]></ST>
		</Algorithm>
	</SimpleFB>
</FBType>
